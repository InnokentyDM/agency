@model IEnumerable<Agency.Models.OBJ>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ADDRESS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GeoLong)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GeoLat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ROOMS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FLOOR)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KIND_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TYPE_ID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ADDRESS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GeoLong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GeoLat)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ROOMS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FLOOR)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KIND_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TYPE_ID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
